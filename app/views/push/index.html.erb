<div id="page-wrapper">
  <div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">发送控制台</h1>
  </div>
  </div>
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          发送消息
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-lg-6">
              <%= form_tag('push/send_go', multipart: true)%>
                <div class="form-group">
                  <label>输入消息体</label>
                  <input class="form-control" placeholder="Enter text" name= "message">
                  <input class="form-control" placeholder="Enter id" name= "userId">
                </div>
                <button type="submit" class="btn btn-default">Submit Button</button>
                <button type="reset" class="btn btn-default">Reset Button</button>
              </from>
         
